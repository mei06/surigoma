<!DOCTYPE html>
<html>
  <head>
    <title>クラッシュアーモンドすりごま</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
  </head>

  <body>
    <div class="top-wrapper">
      <div class="container">
        <div class="main-img">
          <img class="logo" src="<%= "/top_images/logo.png" %>">
          <img class="img01" src="<%= "/top_images/visual_img01.png" %>">
          <img class="img03" src="<%= "/top_images/visual_img03.png" %>">
          <img class="img04" src="<%= "/top_images/visual_img04.png" %>">
          <img class="img05" src="<%= "/top_images/visual_img05.png" %>">
